<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DSP-URBANOCAS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-grid.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    <link href="~/lib/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/lib/boxicons/css/boxicons.min.css" rel="stylesheet">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-dsp border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">DSP-URBANOCAS</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <partial name="_MenuPartial" />  
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Inmuebles" asp-action="Inmuebles" >Inmuebles</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
<footer class="footer">
        <h3 style="color:white;">DSP-URBANOCAS</h3>   
        <div class="social-links">          
          <a href="https://www.facebook.com/alfamarketingdigital97" class="facebook"><i class="bx bxl-facebook"></i></a>
          <a href="https://www.instagram.com/alfa_marketing/?hl=es-la" class="instagram"><i class="bx bxl-instagram"></i></a>   
        <hr size="35px" width="50%" align="center" color="#394665"/> 
            <div class="container footer-bottom clearfix">
      <div class="copyright">
        &copy;DSP-URBANOCAS | Inmobiliaria-Constructora.
      </div>
      <div class="credits">
        Contacto Tel: (01) 999 9999 Cel: 951 999999 
      </div>
       </div>
    </footer>
