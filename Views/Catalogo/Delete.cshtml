@model crmUrbano.Models.Catalogo

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>


<div>
    <h4>Catalogo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
         <h5   @Html.DisplayNameFor(model => model.Name)> Detalle Inmuebles</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
          <h5  @Html.DisplayNameFor(model => model.Direccion)> Direccion</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Direccion)
        </dd>
        <dt class = "col-sm-2">
         <h5   @Html.DisplayNameFor(model => model.Price)> Precio</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
        <dt class = "col-sm-2">
         <h5   @Html.DisplayNameFor(model => model.ImagenName)> Nombre imagen</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ImagenName)
        </dd>
        <dt class = "col-sm-2">
        <h5    @Html.DisplayNameFor(model => model.Status)>Disponibilidad</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Borrar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
