@model crmUrbano.Models.Catalogo

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Catalogo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           <h5 @Html.DisplayNameFor(model => model.Name)> Detalle Inmuebles</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
           <h5 @Html.DisplayNameFor(model => model.Direccion)> Direccion</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Direccion)
        </dd>
        <dt class = "col-sm-2">
          <h5  @Html.DisplayNameFor(model => model.Price)> Precio</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
        <dt class = "col-sm-2">
           <h5 @Html.DisplayNameFor(model => model.ImagenName)> Nobre de imagen</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ImagenName)
        </dd>
        <dt class = "col-sm-2">
          <h5  @Html.DisplayNameFor(model => model.Status)> Disponibilidad</h5>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID">Editar</a> |
    <a asp-action="Index">Volver</a>
</div>
